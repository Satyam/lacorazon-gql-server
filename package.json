{
  "name": "lacorazon-gql-server",
  "version": "1.0.0",
  "description": "GraphQL- Express server for La Coraz√≥n app.",
  "main": "index.js",
  "scripts": {
    "test": "reset; npm run test:json ; npm run test:sqlite",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:json": "DATA_SOURCE=JSON jest",
    "test:sqlite": "DATA_SOURCE=SQLite jest",
    "start": "nodemon --exec ts-node --files src/index.ts",
    "server": "ts-node --files src/index.ts",
    "server:debug": "TS_NODE_FILES=true node -r ts-node/register --inspect src/index.ts",
    "start:json": "DATA_SOURCE=JSON npm start",
    "start:sqlite": "DATA_SOURCE=SQLite npm start",
    "migrate2Json": "babel-node migrate/toJSON.js",
    "migrate2Sqlite": "babel-node migrate/toSQLite.js",
    "clean": "npm run migrate2Json && npm run migrate2Sqlite",
    "lint": "eslint  \"src/**/*.ts\"",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satyam/lacorazon-gql-server.git"
  },
  "keywords": [],
  "author": "Daniel Barreiro <satyam@satyam.com.ar> (Satyam)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satyam/lacorazon-gql-server/issues"
  },
  "homepage": "https://github.com/Satyam/lacorazon-gql-server#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@prisma/cli": "^2.0.0",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "apollo-server-testing": "^2.14.3",
    "axios": "^0.19.2",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.14.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "fs-extra": "^9.0.1",
    "graphql": "^15.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "ms": "^2.1.2",
    "sqlite": "^4.0.10",
    "sqlite3": "^4.2.0"
  }
}
